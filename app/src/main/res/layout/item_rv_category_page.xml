<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/item"
    android:orientation="vertical"
    android:weightSum="4">
<!--    tools:context=".CategoryActivity"-->


    <androidx.cardview.widget.CardView
        android:id="@+id/arrow_jewelry"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80sdp"
        android:layout_weight="1"
        app:cardBackgroundColor="@color/purple_my"
        android:layout_margin="@dimen/_10sdp"
        app:cardCornerRadius="@dimen/_10sdp"
        app:cardElevation="@dimen/_5sdp">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@drawable/buttons_corner_puple">


            <ImageView
                android:id="@+id/img_cat"
                android:layout_width="@dimen/_150sdp"
                android:layout_height="@dimen/_150sdp"
                android:layout_alignParentStart="true"
                android:src="@drawable/ic_jewelry" />

            <TextView
                android:id="@+id/tv_dish_name"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/_28sdp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/comicneue"
                android:maxEms="10"
                android:maxLines="2"
                android:padding="@dimen/_10sdp"
                android:text="Example"
                android:textColor="@color/white"
                android:textSize="@dimen/_14ssp" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_baseline_arrow_forward_ios_24"
                android:layout_alignParentRight="true"
                android:layout_marginTop="38dp"/>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/arrow_electronic"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80sdp"
        android:layout_weight="1"
        app:cardBackgroundColor="@color/purple_my"
        android:layout_margin="@dimen/_10sdp"
        app:cardCornerRadius="@dimen/_10sdp"
        app:cardElevation="@dimen/_5sdp">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@drawable/buttons_corner_puple">


            <ImageView
                android:layout_width="@dimen/_150sdp"
                android:layout_height="@dimen/_150sdp"
                android:layout_alignParentStart="true"
                android:src="@mipmap/electronic" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/_28sdp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/comicneue"
                android:maxEms="10"
                android:maxLines="2"
                android:padding="@dimen/_10sdp"
                android:text="Example"
                android:textColor="@color/white"
                android:textSize="@dimen/_14ssp" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_baseline_arrow_forward_ios_24"
                android:layout_alignParentRight="true"
                android:layout_marginTop="38dp"/>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/arrow_manClothing"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80sdp"
        android:layout_weight="1"
        app:cardBackgroundColor="@color/purple_my"
        android:layout_margin="@dimen/_10sdp"
        app:cardCornerRadius="@dimen/_10sdp"
        app:cardElevation="@dimen/_5sdp">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@drawable/buttons_corner_puple">


            <ImageView
                android:layout_width="@dimen/_150sdp"
                android:layout_height="@dimen/_150sdp"
                android:layout_alignParentStart="true"
                android:src="@mipmap/man" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/_28sdp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/comicneue"
                android:maxEms="10"
                android:maxLines="2"
                android:padding="@dimen/_10sdp"
                android:text="Example"
                android:textColor="@color/white"
                android:textSize="@dimen/_14ssp" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_baseline_arrow_forward_ios_24"
                android:layout_alignParentRight="true"
                android:layout_marginTop="38dp"/>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/arrow_womenClothing"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80sdp"
        android:layout_weight="1"
        app:cardBackgroundColor="@color/purple_my"
        android:layout_margin="@dimen/_10sdp"
        app:cardCornerRadius="@dimen/_10sdp"
        app:cardElevation="@dimen/_5sdp">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@drawable/buttons_corner_puple">


            <ImageView
                android:layout_width="@dimen/_150sdp"
                android:layout_height="@dimen/_150sdp"
                android:layout_alignParentStart="true"
                android:src="@mipmap/man" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/_28sdp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/comicneue"
                android:maxEms="10"
                android:maxLines="2"
                android:padding="@dimen/_10sdp"
                android:text="Example"
                android:textColor="@color/white"
                android:textSize="@dimen/_14ssp" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_baseline_arrow_forward_ios_24"
                android:layout_alignParentRight="true"
                android:layout_marginTop="38dp"/>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

</LinearLayout>

